  <!-- EN: recruit attempt -->
  <RecruitAttempt.label>Anwerbungsgespräch</RecruitAttempt.label>
  <!-- EN:
    <li>r_logentry->[INITIATOR_nameDef] asked [RECIPIENT_nameDef] to join.</li>
    <li>r_logentry->[INITIATOR_nameDef] promised [goodthing] to [RECIPIENT_nameDef], and asked [RECIPIENT_objective] to join.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->free, fresh [Vegetable]s</li>
  -->
  <RecruitAttempt.logRulesInitiator.rulesStrings>
    <li>r_logentry->[INITIATOR_nameDef] versuchte [RECIPIENT_nameDef] anzuwerben.</li>
    <li>r_logentry->[INITIATOR_nameDef] versprach [goodthing] für [RECIPIENT_nameDef] und fragte ob [RECIPIENT_pronoun] mitmachen will.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->[VegetablePlural], frisch und kostenlos</li>
  </RecruitAttempt.logRulesInitiator.rulesStrings>
  
  <!-- EN: spark jailbreak -->
