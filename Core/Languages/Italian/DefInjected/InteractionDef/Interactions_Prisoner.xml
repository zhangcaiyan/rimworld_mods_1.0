  <!-- EN: recruit attempt -->
  <RecruitAttempt.label>tentativo di reclutamento</RecruitAttempt.label>
  <!-- EN:
    <li>r_logentry->[INITIATOR_nameDef] asked [RECIPIENT_nameDef] to join.</li>
    <li>r_logentry->[INITIATOR_nameDef] promised [goodthing] to [RECIPIENT_nameDef], and asked [RECIPIENT_objective] to join.</li>
    <li>goodthing(p=15)->[ConceptPositive]</li>
    <li>goodthing->free, fresh [Vegetable]s</li>
  -->
  <RecruitAttempt.logRulesInitiator.rulesStrings>
    <li>r_logentry->[INITIATOR_nameDef] ha chiesto a [RECIPIENT_nameDef] di unirsi.</li>
    <li>r_logentry->[INITIATOR_nameDef] ha promesso a [RECIPIENT_nameDef] dei [goodthingMas], chiedendo a [RECIPIENT_objective] di unirsi.</li>
    <li>r_logentry->[INITIATOR_nameDef] ha promesso a [RECIPIENT_nameDef] delle [goodthingFem], chiedendo a [RECIPIENT_objective] di unirsi.</li>
    <li>r_logentry->[INITIATOR_nameDef] ha promesso a [RECIPIENT_nameDef] degli [goodthingNeu], chiedendo a [RECIPIENT_objective] di unirsi.</li>
    <li>goodthingFem(p=15)->[ConceptPositivePluralFem]</li>
    <li>goodthingNeu(p=15)->[ConceptPositivePluralNeu]</li>
    <li>goodthingMas->[VegetablePluralMas] freschi gratis</li>
    <li>goodthingFem->[VegetablePluralFem] fresche gratis</li>
  </RecruitAttempt.logRulesInitiator.rulesStrings>
  
