      <!-- 野生度。調教しやすさ、手なづけやすさに影響。床を汚す度合いにも影響しているかも-->
      <wildness>0.2</wildness>
      <nuzzleMtbHours>24</nuzzleMtbHours>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, 1)</li>
          <li>(2, 1.7)</li>
          <li>(3, 0.7)</li>
          <li>(3.5, 0)</li>
        </points>
      </litterSizeCurve>
      <!--寿命、年齢が高い個体は傷跡や病気にかかっている可能性が上がります -->
      <lifeExpectancy>15</lifeExpectancy>
