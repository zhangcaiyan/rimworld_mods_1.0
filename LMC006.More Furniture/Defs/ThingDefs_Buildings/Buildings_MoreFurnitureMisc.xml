<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  

<!--============================== Other Facilities ==============================-->  

<!--====================== file cabinet ======================-->  
  
  <ThingDef ParentName="Anon2NoCompQualityFurnitureBase">
    <defName>Anon2FileCabinet</defName>
    <label>file cabinet</label>
    <description>An organizational tool often used for keeping records in old fashion paper form.</description>
    <graphicData>
      <texPath>Things/Building/Furniture/Misc/Anon2FileCabinet2</texPath>
	  <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
      <damageData>
        <rect>(0.15,0.15,1.7,1.7)</rect>
      </damageData>
    </graphicData>
    <size>(1,1)</size>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <minifiedDef>MinifiedThing</minifiedDef>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <WorkToBuild>1200</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>5</Beauty>
      <Mass>15</Mass>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
    <comps>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <ResearchSpeedFactor>0.05</ResearchSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
      </li>
    </comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
    <designationCategory>ANON2MF</designationCategory>
    <defaultPlacingRot>South</defaultPlacingRot>
    <techLevel>Medieval</techLevel>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
  </ThingDef>

  
