<Defs>

<!--============================== Seating ==============================-->

<!--====================== square dining chair ======================-->  


  <ThingDef ParentName="Anon2ArtableFurnitureBase">
    <defName>Anon2SquareDiningChair</defName>
    <label>方形餐椅</label>
    <graphicData>
      <texPath>Things/Building/Furniture/SeatingStuff/Anon2SquareChair</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3,3)</drawSize>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
      <damageData>
        <rectN>(0.25,0.234375,0.5,0.28125)</rectN>
        <rectS>(0.25,0.234375,0.5,0.28125)</rectS>
        <rectE>(0.28125,0.25,0.453125,0.25)</rectE>
        <rectW>(0.28125,0.25,0.453125,0.25)</rectW>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <statBases>
      <MaxHitPoints>170</MaxHitPoints>
      <WorkToBuild>6000</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>7</Beauty>
      <Comfort>0.70</Comfort>
    </statBases>
    <description>一个舒适又迷人的餐椅。</description>
    <socialPropernessMatters>true</socialPropernessMatters>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>45</costStuffCount>
    <pathCost>30</pathCost>
    <fillPercent>0.35</fillPercent>
    <building>
      <isSittable>true</isSittable>
    </building>
    <designationCategory>ANON2MF</designationCategory>
    <defaultPlacingRot>South</defaultPlacingRot>
    <techLevel>Medieval</techLevel>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
  </ThingDef>


