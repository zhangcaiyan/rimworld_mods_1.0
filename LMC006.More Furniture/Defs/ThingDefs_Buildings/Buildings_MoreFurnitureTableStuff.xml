<!--====================== table (tiny) ======================-->  

  <ThingDef ParentName="TableGatherSpotBase">
    <defName>Anon2TableTiny</defName>
    <label>table (1x1)</label>
    <graphicData>
      <texPath>Things/Building/Furniture/TableStuff/Anon2TableTiny</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,3)</drawSize>
      <damageData>
        <rect>(0.1,0.29,0.8,0.7)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <statBases>
      <MaxHitPoints>30</MaxHitPoints>
      <WorkToBuild>600</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>3</Beauty>
      <Mass>4</Mass>
    </statBases>
    <size>(1,1)</size>
    <costStuffCount>15</costStuffCount>
    <rotatable>false</rotatable>
    <designationCategory>ANON2MF</designationCategory>
  </ThingDef>


  <!--========================= Counter Basic =============================-->



  <ThingDef ParentName="Anon2TableBase">
    <defName>Anon2CounterA</defName>
    <label>Counter</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <description>An Counter, has a large flat top.</description>
    <graphicData>
      <texPath>Things/Building/Furniture/TableStuff/Linked/Anon2Counter_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <linkType>CornerFiller</linkType>
      <linkFlags>
        <li>Custom6</li>
      </linkFlags>
      <damageData>
        <rectN>(0.2,0.2,0.2,0.2)</rectN>
        <rectS>(0.2,0.2,0.2,0.2)</rectS>
        <rectE>(0.2,0.2,0.2,0.2)</rectE>
        <rectW>(1,1,1,1)</rectW>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <uiIconPath>Things/Building/Furniture/TableStuff/Linked/Anon2Counter_MenuIcon</uiIconPath>
    <statBases>
      <MaxHitPoints>180</MaxHitPoints>
      <WorkToBuild>600</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>2</Beauty>
      <Mass>20</Mass>
    </statBases>    
    <costList>
      <WoodLog>4</WoodLog>
    </costList>
    <costStuffCount>8</costStuffCount>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <blockWind>false</blockWind>
    <coversFloor>false</coversFloor>
    <placingDraggableDimensions>1</placingDraggableDimensions>
    <tickerType>Never</tickerType>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>false</neverMultiSelect>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <holdsRoof>false</holdsRoof>
    <staticSunShadowHeight>0.8</staticSunShadowHeight>
    <blockLight>false</blockLight>
    <altitudeLayer>Building</altitudeLayer>
    <pathCost>40</pathCost>
    <pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.65</fillPercent>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <building>
      <isInert>true</isInert>
    <blueprintGraphicData>
      <texPath>Things/Building/Furniture/TableStuff/Linked/Anon2Counter_Blueprint_Atlas</texPath>
    </blueprintGraphicData>
    </building>
    <damageMultipliers>
      <li>
        <damageDef>Bomb</damageDef>
        <multiplier>1.5</multiplier>
      </li>
    </damageMultipliers>
    <designationCategory>ANON2MF</designationCategory>
  </ThingDef>


