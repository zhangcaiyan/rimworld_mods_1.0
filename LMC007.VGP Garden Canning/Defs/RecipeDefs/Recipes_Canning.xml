<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<RecipeDef>
	<defName>Canmeal</defName>
	<label>can meal</label>
	<description>Preserve food in a can to prevent spoiling.</description>
	<jobString>Canning food.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<workAmount>1000</workAmount>
		<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>          <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>FoodRaw</li>
          </categories>
        </filter>
        <count>5</count>
      </li>
	</ingredients>
	<fixedIngredientFilter>
	  <categories>
		<li>FoodRaw</li>
	  </categories>
      <specialFiltersToDisallow>
        <li>AllowPlantFood</li>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
      <disallowedCategories>
         <li>CookingSupplies</li>
     </disallowedCategories>
	</fixedIngredientFilter>
	<products>
	  <MealCannedFood>7</MealCannedFood>
	</products>
    <defaultIngredientFilter>
      <categories>
        <li>FoodRaw</li>
      </categories>
      <disallowedCategories>
        <li>EggsFertilized</li>
     </disallowedCategories>
      <disallowedThingDefs>
		<li>Meat_Human</li>
		<li>Meat_Megaspider</li>
		<li>InsectJelly</li>
         	<li>CannedVeg</li>
         	<li>CannedMeat</li>
        	<li>CannedFruit</li>
  </disallowedThingDefs>
    </defaultIngredientFilter>
		<skillRequirements>
			<Cooking>4</Cooking>
		</skillRequirements>
	<workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>CanningFactory</li>
     </recipeUsers>
</RecipeDef>

