<?xml version="1.0" encoding="UTF-8"?>

<Defs>
   
  <ThingDef ParentName="VG_MealBase">
    <defName>MealCannedFood</defName>
    <label>canned meal</label>
    <description>A meal preserved in cans to prevent it going bad.</description>
    <stackLimit>75</stackLimit>
    <graphicData>
      <texPath>Things/Item/Meal/Cannedfood</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>17</MarketValue>
      <Mass>0.4</Mass>
       <Nutrition>0.75</Nutrition>
   </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <thingCategories>
      <li>FoodMeals</li>
    </thingCategories>
  </ThingDef>

  <ThingDef ParentName="VG_ResourceBase">
    <defName>CannedVeg</defName>
    <label>canned vegetables</label>
    <description>Vegetables that have been cooked and preserved in a can to prevent spoilage.</description>
    <stackLimit>400</stackLimit>
    <graphicData>
      <texPath>Things/Item/canveg</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
		<socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <MarketValue>5</MarketValue>
      <Mass>0.2</Mass>
       <Nutrition>0.05</Nutrition>
   </statBases>
    <thingCategories>
      <li>PlantFoodRaw</li>
    </thingCategories>
    <ingestible>
	<foodType>Meal</foodType>
	<preferability>MealSimple</preferability>
			<tasteThought></tasteThought>
      <maxNumToIngestAtOnce>6</maxNumToIngestAtOnce>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Ingredients" />
      <li Class="CompProperties_FoodPoisonable" />
    </comps>
  </ThingDef>

