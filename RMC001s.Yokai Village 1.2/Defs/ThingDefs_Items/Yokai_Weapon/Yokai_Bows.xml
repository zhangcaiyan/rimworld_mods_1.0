      <ImmunityGainSpeed>0.1</ImmunityGainSpeed>
    </equippedStatOffsets>
    <costList>
      <Yokai_JorouGumoThread>20</Yokai_JorouGumoThread>
      <Yokai_Fragment>50</Yokai_Fragment>
